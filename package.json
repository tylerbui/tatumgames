{
  "name": "tatum",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "build": "turbo build",
    "build:types": "turbo build:types",
    "build:packages": "turbo build --filter=./packages/*...",
    "build:vercel": "turbo build:vercel",
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=@tatum/web...",
    "dev:mobile": "turbo dev --filter=@tatum/mobile...",
    "dev:packages": "turbo dev --filter=./packages/*...",
    "dev:all": "turbo dev --parallel",
    "lint": "turbo lint --parallel",
    "check-types": "turbo check-types",
    "check-types:packages": "turbo check-types --filter=./packages/*...",
    "precommit": "pnpm run lint && turbo run build:types test",
    "test": "turbo test",
    "turbo": "turbo",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "husky": "9.1.7",
    "jest": "^30.0.5",
    "prettier": "3.4.2",
    "ts-jest": "^29.4.1",
    "turbo": "2.3.3",
    "typescript": "5.7.2"
  }
}
